        <Compute Id="UpdatedAt" Title="Updated" Expr="@(DateTimeOffset.FromUnixTimeSeconds((long)Model.Inp))" Converter="DateTime" Checked="false">
          <JsonPath Expr="updated_at" Id="Inp" Converter="Long"/>
