          </Compute.Expr>
          <JsonPath Expr="id" Id="Inp"/>
        </Compute>
        <JsonPath Expr="specificContent.*.shareMediaCategory" Id="Category" Title="Category" Converter="String"/>
        <JsonPath Expr="lifecycleState" Id="State" Title="State" Converter="String"/>
        <JsonPath Expr="origin" Id="Origin" Title="Origin" Converter="String"/>
        <JsonPath Expr="versionTag" Id="Version" Title="Version" Converter="Long"/>
        <JsonPath Expr="specificContent.*.media[0].thumbnails[0].url" Id="Thumbnail" Title="Thumbnail" Converter="String" DefaultValue=""/>
        <JsonPath Expr="author" Id="Author" Title="Author" Converter="String"/>
        <JsonPath Expr="created.actor" Id="Actor" Title="Actor" Converter="String"/>
      </JsonPath>
    </Parse>
