<?xml version="1.0" encoding="utf-8" ?>
<Suite Category="SEO" SubCategory="Google" Title="Google CacheDate" Id="GoogleCacheDate" SourceUrl="https://github.com/nielsbosma/SeoTools-for-Excel-Connectors/blob/master/GoogleCacheDate.xml" HelpUrl="http://seotoolsforexcel.com/GoogleCacheDate/" HelpText="Documentation">

  <Author Name="Victor Sandberg" Url="http://community.seotoolsforexcel.com/users/diskborste/activity"/>

  <RestConnector Id="GoogleCacheDate" Title="Google CacheDate" AvailableInSpider="true">
    <Parameters>
      <Text Id="Url" Title="URL" Required="true" DefaultValue="" Debug.DefaultValue="http://www.seotoolsforexcel.com"/>
      <Number Id="Delay" Title="Delay (Milliseconds)" DefaultValue="500" Minimum="0" Maximum="20000"/>
    </Parameters>
		<Fetch>
			<HttpSettings>
				<IntervalBetweenRequests RandomFrom="@(Model.Delay)" RandomTo="@(Model.Delay+10)" IfSame="Domain"/>
			</HttpSettings>
			<Fetch.Url>
				<![CDATA[
				http://webcache.googleusercontent.com/search?q=cache:
				@(Utils.UrlEncode(Model.Url))
				&strip=1&hl=en
				]]>
      </Fetch.Url>
    </Fetch>
    <Parse>
      <Regex Expr="snapshot of the page as it appeared on ([A-Za-z0-9 :,]+GMT)" Id="Date" Title="Date" Converter="DateTime" HelpText=""/>
    </Parse>
  </RestConnector>

</Suite>
