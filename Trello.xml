      <Fail>
        <Regex Expr="^invalid token$" Group="0"/>
        <Regex Expr="^board not found$" Group="0"/>
        <Regex Expr="^invalid id$" Group="0"/>
        <Regex Expr="^model not found$" Group="0"/>
        <Regex Expr="^Cannot GET .+$" Group="0"/>
