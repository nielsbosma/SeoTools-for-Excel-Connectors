        <JsonPath Expr="Description" Id="Description" Title="Description" Converter="String"/>
        <JsonPath Expr="Rating" Id="Rating" Title="Rating" Converter="String"/>
        <JsonPath Expr="CreatedDate" Id="CreatedDate" Title="Created" Converter="String"/>
        <JsonPath Expr="LastModifiedDate" Id="LastModifiedDate" Title="Last Modified" Converter="String"/>
        <JsonPath Expr="UpsellOpportunity" Id="UpsellOpportunity" Title="UpsellOpportunity" Converter="AUto"/>
