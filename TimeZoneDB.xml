				<JsonPath Expr="nextAbbreviation" Id="NextAbbrevation" Title="Next Abbrevation" Converter="String" Checked="true" HelpText=""/>
    </Parse>
    <Fail>
      <JsonPath Expr="message"/>
    </Fail>
