  <RazorFunctions>
    <![CDATA[
      string Fetchkeywords() {
				string[] lines = ((string)Model.Ids).Trim().Split('\n').Select(e => e.Trim()).Where(e => !string.IsNullOrEmpty(e)).ToArray();
        return string.Join("&keyword[]=",lines.Select((e, i) => Utils.UrlEncode(e)).ToArray());
      }
    ]]>
  </RazorFunctions>

